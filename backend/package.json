{
    "name": "conference-backend",
    "version": "0.1.0",
    "main": "dist/index.js",
    "scripts": {
        "clean": "rimraf dist",
        "build": "npm run clean && tsc",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "test": "vitest",
        "typecheck": "tsc --noEmit",
        "start": "node dist/index.js",
        "db:push": "drizzle-kit push",
        "db:generate": "drizzle-kit generate",
        "studio": "DB_HOST=127.0.0.1 drizzle-kit studio --host=local.drizzle.studio --port=3337"
    },
    "dependencies": {
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "drizzle-orm": "^0.44.1",
        "express": "^4.18.2",
        "gel": "^2.1.1",
        "mysql2": "^3.0.0",
        "tslog": "^4.9.1"
    },
    "devDependencies": {
        "@tanstack/react-query-devtools": "^5.79.0",
        "@types/cors": "^2.8.18",
        "@types/express": "^5.0.2",
        "@types/node": "^24.1.0",
        "drizzle-kit": "^0.31.1",
        "rimraf": "^6.0.1",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.8.3",
        "vitest": "^3.2.4"
    }
}
