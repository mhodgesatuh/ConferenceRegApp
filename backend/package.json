{
    "name": "conference-backend",
    "version": "0.1.0",
    "main": "dist/index.js",
    "scripts": {
        "clean": "rimraf dist",
        "build": "npm run clean && tsc",
        "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
        "start": "NODE_ENV=production node -r module-alias/register dist/index.js",
        "test": "vitest",
        "typecheck": "tsc --noEmit",
        "db:push": "drizzle-kit push",
        "db:generate": "drizzle-kit generate",
        "studio": "DB_HOST=127.0.0.1 drizzle-kit studio --host=127.0.0.1 --port=3337"
    },
    "_moduleAliases": {
        "@": "dist"
    },
    "dependencies": {
        "body-parser": "^1.20.2",
        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "drizzle-orm": "^0.44.1",
        "express": "^4.18.2",
        "gel": "^2.1.1",
        "helmet": "^8.1.0",
        "module-alias": "^2.2.3",
        "mysql2": "^3.0.0",
        "supertest": "^7.1.4",
        "tslog": "^4.9.1",
        "typescript": "5.8.3"
    },
    "devDependencies": {
        "@types/compression": "^1.8.1",
        "@types/cors": "^2.8.18",
        "@types/express": "^5.0.2",
        "@types/node": "^24.1.0",
        "drizzle-kit": "^0.31.1",
        "rimraf": "^6.0.1",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.8.3",
        "vite": "^6.3.5",
        "vitest": "^3.2.4"
    }
}
